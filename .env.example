# Banco de Dados PostgreSQL
DATABASE_URL="postgresql://usuario:senha@localhost:5432/clinica_agenda?schema=public"

# Chave secreta para JWT (gere uma chave segura em produção)
JWT_SECRET="sua-chave-secreta-muito-segura-aqui-mude-em-producao"

# URL base da aplicação
NEXT_PUBLIC_APP_URL="http://localhost:3000"

# Webhook N8N (URLs do seu workflow N8N)
N8N_WEBHOOK_URL="https://seu-n8n.com/webhook"
N8N_WEBHOOK_SECRET="sua-chave-secreta-webhook"

# Google Calendar API
# Obtenha em: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID="seu-client-id.apps.googleusercontent.com"
GOOGLE_CLIENT_SECRET="seu-client-secret"
GOOGLE_REDIRECT_URI="http://localhost:3000/api/google/callback"

# Ambiente
NODE_ENV="development"
